{% extends "@SurvosBootstrap/sneat/" ~ layout|default('base') ~ ".html.twig" %}
{#{% extends "@SurvosBootstrap/tabler/layout-" ~ layout|default('vertical') ~ ".html.twig" %}#}


{% set appShortName = 'KPA' %}


{% block logo_mini %}<b>KPA</b>{% endblock %}
{% block logo_large '<b>KPA</b> Admin' %}

{% block page_title 'KPA Admin' %}
{% block page_subtitle 'Songs and Music!' %}

    {% block stylesheets %}
        {# always include app?  Or add it to stylesheets block and always call parent?
        #}
        {{ parent() }}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {# always include app?  Or add it to stylesheets block and always call parent?
        #}
        {{ parent() }}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}

{# could also overwrite template
        {% include '@SurvosBootstrapBundle/Partials/_footer.html.twig' %}
#}

{% block footer %}
    <footer class="main-footer">
        <!-- To the right -->
        <div class="pull-right hidden-xs">

        </div>
