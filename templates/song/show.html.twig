
    {% set slotOptions = {
        NAVBAR_MENU: {
            x: null,
            frontPage: true
        },
        PAGE_ACTIONS: {
            song: song,
        },
        BREADCRUMBS: {
            song: song
        }
    } %}
    <twig:tabler:page :title="song.title"
                      :slotOptions="slotOptions"
    >
    <div class="row">
    <div class="col-md-6">
        {% for video in song.videos %}
            <object width="420" height="315"
                    data="https://www.youtube.com/embed/{{ video.youtubeId }}">
            </object>
        {% endfor %}
    </div>
    <div class="col-md-6">
        {# reused accordion? #}
        {{ component('cards:tabs', { active: 'profile',
            tabs: [
                tab('profile', '@profile data', translationDomain='fosUserBundle'),
                tab('school', content=song.school),
            ] }) }}
    </div>
</div>


    <twig:divider />

    <h1>{{ song.title }} ({{ song.school }}, {{ song.year }})</h1>

    {# Music Sheet Display #}
    <div class="row mb-4">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Sheet Music</h3>
                </div>

                <div class="card-body">
                    {% set _sc = 'music' %}
                    {% set url = asset('blinked.xml') %}
{#                    {% set url = asset('coffee-complex.cho') %}#}
                    <a href="{{ url }}" target="_blank">{{ url }}</a>
                    <div {{ stimulus_controller(_sc, {
                        'url': url,
                        'format': 'xml',
                        'width': 800,
                        'height': 600
                    }) }} style="min-height: 400px; border: 1px solid #dee2e6; border-radius: 0.375rem; padding: 1rem;"
                          class="text-center text-muted">
                        <div class="spinner-border" role="status">
                            <span class="visually-hidden">Loading music...</span>
                        </div>
                        <p class="mt-2">Loading sheet music...</p>
                        <div {{ stimulus_target(_sc, 'display') }}>
                            Rendered music goes here.
                        </div>
                    </div>
                </div>

                <div id="osmdContainer">
                    xml here
                </div>


{#                {% for ext in ['xml','cho'] %}#}
{#                    <div class="card-body">#}
{#                        {% set _sc = 'music-display' %}#}
{#                        {% set url = asset('abuelo.' ~ ext) %}#}
{#                        <a href="{{ url }}" target="_blank">{{ url }}</a>#}
{#                        <div {{ stimulus_controller(_sc, {#}
{#                            'url': url,#}
{#                            'width': 800,#}
{#                            'height': 600#}
{#                        }) }} style="min-height: 400px; border: 1px solid #dee2e6; border-radius: 0.375rem; padding: 1rem;"#}
{#                              class="text-center text-muted">#}
{#                            <div class="spinner-border" role="status">#}
{#                                <span class="visually-hidden">Loading music...</span>#}
{#                            </div>#}
{#                            <p class="mt-2">Loading sheet music...</p>#}
{#                        </div>#}
{#                    </div>#}

{#                {% endfor %}#}
            </div>
        </div>
    </div>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ song.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ song.title }}</td>
            </tr>
            <tr>
                <th>Youtube Videos</th>
                <td>

                </td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ song.date ? song.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Year</th>
                <td>{{ song.year }}</td>
            </tr>
            <tr>
                <th>School</th>
                <td>{{ song.school }}</td>
            </tr>
            <tr>
                <th>Lyrics</th>
                <td>
                    <pre>
                    {{ song.lyrics }}
                    </pre>
                </td>
            </tr>
            <tr>
                <th>FeaturedArtist</th>
                <td>{{ song.featuredArtist }}</td>
            </tr>
            <tr>
                <th>RecordingCredits</th>
                <td>{{ song.recordingCredits }}</td>
            </tr>
            <tr>
                <th>Musicians</th>
                <td>{{ song.musicians }}</td>
            </tr>
            <tr>
                <th>Writers</th>
                <td>{{ song.writers }}</td>
            </tr>
            <tr>
                <th>WordpressPageId</th>
                <td>{{ song.wordpressPageId }}</td>
            </tr>
            <tr>
                <th>Recording</th>
                <td>{{ song.recording }}</td>
            </tr>
            <tr>
                <th>Publisher</th>
                <td>{{ song.publisher }}</td>
            </tr>
{#            <tr>#}
{#                <th>Notes</th>#}
{#                <td>{{ song.notes }}</td>#}
{#            </tr>#}
        </tbody>
    </table>

    </twig:tabler:page>
