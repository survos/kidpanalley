{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-zip": "*",
        "api-platform/core": "^2.5",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "*",
        "doctrine/doctrine-migrations-bundle": "*",
        "doctrine/orm": "*",
        "easycorp/easyadmin-bundle": "^2",
        "google/apiclient": "^2.0@dev",
        "league/oauth2-facebook": "^2.0",
        "league/oauth2-github": "^2.0",
        "league/oauth2-google": "^3.0",
        "mindscreen/yarnlock": "^1.0",
        "nelmio/cors-bundle": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpoffice/phpword": "dev-master",
        "sensio/framework-extra-bundle": "^5.2",
        "survos/base-bundle": "dev-master",
        "symfony/asset": "^5.2",
        "symfony/console": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/expression-language": "^5.2",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/http-client": "^5.2",
        "symfony/intl": "^5.2",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "^5.2",
        "symfony/property-access": "5.2.*",
        "symfony/property-info": "5.2.*",
        "symfony/proxy-manager-bridge": "5.2.*",
        "symfony/security-bundle": "^5.2",
        "symfony/serializer": "5.2.*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^5.2",
        "symfony/twig-bundle": "^5.2",
        "symfony/validator": "^5.2",
        "symfony/web-link": "^5.2",
        "symfony/yaml": "^5.2",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "yectep/phpspreadsheet-bundle": "*@dev"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "repositories": {
        "adminlte": {
            "type": "vcs",
            "url": "git@github.com:tacman/AdminLTEBundle.git"
        },
        "survosbase": {
            "type": "vcs",
            "url": "git@github.com:survos/BaseBundle.git"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "doctrine:migrations:migrate -n": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.2.*"
        }
    }
}
