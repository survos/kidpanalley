{
    "name": "tacman/kpa",
    "description": "Archives for Kid Pan Alley",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-zip": "*",
        "api-platform/core": "^2.5",
        "composer/package-versions-deprecated": "^1.11.99",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "*",
        "doctrine/doctrine-migrations-bundle": "*",
        "doctrine/orm": "*",
        "easycorp/easyadmin-bundle": "*",
        "google/apiclient": "^2.0",
        "league/oauth2-facebook": "^2.0",
        "league/oauth2-github": "^3.0",
        "league/oauth2-google": "^4.0",
        "mindscreen/yarnlock": "^1.0",
        "nelmio/cors-bundle": "^2.1",
        "phpoffice/phpword": "dev-master",
        "sensio/framework-extra-bundle": "^6.2",
        "survos/base-bundle": "*@dev",
        "symfony/asset": "^6.0",
        "symfony/console": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/expression-language": "^6.0",
        "symfony/flex": "^v2.2.2",
        "symfony/form": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/intl": "^6.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "^6.0",
        "symfony/property-access": "^6.0",
        "symfony/property-info": "^6.0",
        "symfony/proxy-manager-bridge": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/translation": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/web-link": "^6.0",
        "symfony/yaml": "^6.0",
        "symfonycasts/verify-email-bundle": "^1.11",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "yectep/phpspreadsheet-bundle": "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^6.0",
        "symfony/css-selector": "^6.0",
        "symfony/debug-bundle": "^6.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "^6.0",
        "symfony/web-profiler-bundle": "^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "repositories": {
        "base": {
            "type": "path",
            "url": "//home/tac/survos/bundles/base-bundle"
        },
        "survos_base_bundle": {
            "type": "vcs",
            "url": "git@github.com:survos/BaseBundle.git"
        },
        "adminlte": {
            "type": "vcs",
            "url": "git@github.com:tacman/AdminLTEBundle.git"
        },
        "survosbase": {
            "type": "vcs",
            "url": "git@github.com:survos/BaseBundle.git"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "doctrine:migrations:migrate -n": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^6.0"
        }
    }
}
